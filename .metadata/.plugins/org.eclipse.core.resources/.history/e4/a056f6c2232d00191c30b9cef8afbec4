
public class Matrice {
	
	public static int[][] minus(int[][]m1,int[][] m2 ) {
		int[][] ms = new int[m1.length][m1[0].length ];
		  for ( int i = 0 ; i < m1.length ; i++ ) {
		    for ( int j = 0 ; j < m1[0].length ; j++ ) {
		    	
		      ms[i][j] = m1[i][j]- m2[i][j]; } 
		 
		}
	
		  return ms;
	

	

	}
}
